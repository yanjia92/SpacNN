
ctmc

const int cntNa = 10;
const int cntCl = 10;

module Na
na : [0, cntNa] init cntNa;
[e1] na > 0 -> na : (na' = na - 1);
[e1] na < cntNa -> (cntNa - na) : (na' = na + 1);
endmodule

module Cl
cl : [0, cntCl] init cntCl;
[e1] cl > 0 -> cl : (cl' = cl - 1);
[e2] cl < cntCl -> (cntCl - cl) : (cl' = cl + 1);
endmodule

const double e1_rate = 100;
const double e2_rate = 10;

module baserate
[e1] true -> e1_rate : true;
[e2] true -> e2_rate : true;
endmodule

label naeq5 = na == 5;


